\n            this.setState({\n            projects: arr\n            })\n        })\n\n        fetch('http://localhost:3000/users')\n        .then(response => {\n            return response.json()\n        })\n        .then(arr => {\n            this.setState({\n            users: arr\n            })\n        })\n    }\n\n    // loginUser = (userObj) => {\n    //     this.setState({currentUser: userObj})\n    // }\n\n    loginUser = (userObj) => {\n        this.props.loginUser(userObj)\n    }\n\n    \n    \n    render() {\n        console.log(this.state)\n        return (\n            //comment back in after testing \n            <div>\n                {!this.props.currentUser ?\n                <>\n                    <Login users={this.state.users} loginUser={this.loginUser}/>\n                    OR\n                    <Signup users={this.state.users} loginUser={this.loginUser}/>\n                </>\n                    : \n                    <Projects  projects={this.state.projects} users={this.state.users} currentUser={this.state.currentUser}/>\n                }\n                {/* <Projects  projects={this.state.projects} users={this.state.users} currentUser={this.state.currentUser}/> */}\n            </div>\n        )\n    }\n}\n\nexport default MainContainer\n","/Users/jonathanhakimian/Desktop/Flatiron/Mod_5_Project/frontend/src/components/Project.js",["85"],"\nimport React, { Component } from 'react'\nimport {Link} from 'react-router-dom'\nimport ProjectShow from './ProjectShow'\n\n\nclass Project extends Component {\n\n    render() {\n        return (\n            <div>\n                <Link to={`/projects/${this.props.project.id}`} id={this.props.project.id} currentUser={this.props.currentUser}>\n                    <div className=\"project\">\n                        <h1>{this.props.project.developer_name}</h1>\n                        <h2>Location: {this.props.project.location}</h2>\n                        <h3>Total Capital Needed: {this.props.project.total_capital_needed}</h3>\n                        <h4>Minimum Investment: {this.props.project.minimum_investment}</h4>\n                        <h4>Projected ROI: {this.props.project.projected_roi}</h4>\n                    </div>\n                </Link>\n            </div>\n        )\n    }\n}\n\nexport default Project\n",["86","87"],"/Users/jonathanhakimian/Desktop/Flatiron/Mod_5_Project/frontend/src/components/ProjectShow.js",["88","89","90","91"],"\nimport React, { Component } from 'react'\nimport {useParams} from 'react-router-dom'\nimport InvestmentForm from './InvestmentForm'\nimport { Line, Circle } from 'rc-progress'\nimport ProjectImageSlider from './ProjectImageSlider'\nimport {Link} from 'react-router-dom'\n\nclass ProjectShow extends React.Component  {\n    \n    state = {\n        project: null,\n        investClicked: false,\n        investments: []\n    }\n\n    \n    componentDidMount() {\n        fetch(`http://localhost:3000/projects/${this.props.match.params.id}`) //this.routerProps.match.params.id\n        .then(response => {\n            return response.json()\n        })\n        .then(data => this.setState({project: data, investments: data.investments}))\n        \n    }\n\n    capital_raised = () => {\n        let total = 0\n        if (this.state.project) {\n            let investments = this.state.investments\n            investments.forEach(element => {\n                total += element.amount\n            });\n        }\n        return total\n    }\n\n    localClickHandler = () => {\n        this.setState({investClicked: !this.state.investClicked})\n    }\n\n    projectInvestments = () => {\n        let sortedInvestments = this.state.investments.sort((a, b) => b.id - a.id)\n        return sortedInvestments.map(inv => \n            <li>           \n                {inv.date} \n                <Link to={`/users/${inv.user.id}`} key={inv.id}>{inv.user.username}</Link> \n                Amount: ${inv.amount}\n                {this.props.currentUser.id === inv.user.id ?\n                    <button onClick={this.onDelete} value={inv.id}>Delete Investment</button>\n                    : null\n                }\n            </li>\n        )\n    }\n\n    onDelete = (e) => {  \n        let id = e.target.value\n        let newInvestments = this.state.investments.filter(inv => inv.id != id)\n        this.setState({investments: newInvestments})\n\n        fetch(`http://localhost:3000/investments/${id}`, {\n            method: \"DELETE\",\n        })\n    }\n\n    percent = () => {\n        let percent \n        percent = (this.capital_raised()) / (this.state.project.total_capital_needed) * 100 \n        return percent.toFixed(2).toString()\n    }\n\n    addInvestment = (investmentObj) => {\n        this.setState({investments: [investmentObj, ...this.state.investments]})\n    }\n\n    render() {\n        console.log(this.props)\n\n        return (\n            <div>\n            {this.state.project ?  \n            <div>\n                    <h1>Project Show</h1>\n                    <ProjectImageSlider project={this.state.project}/>\n                    <h1>{this.state.project.developer_name}</h1>\n                    <h2>Location: {this.state.project.location}</h2>\n                    <h3>Total Capital Needed: {this.state.project.total_capital_needed}</h3>\n                    <h4>Percent raised: {this.percent()} %</h4>\n                    <Line percent={this.percent()} strokeWidth=\"4\" strokeColor=\"green\" trailWidth=\"4\" />\n                    <h4>Captial Raised: {this.capital_raised()}</h4>\n                    <h4>Minimum Investment: {this.state.project.minimum_investment}</h4>\n                    <h4>Projected ROI: {this.state.project.projected_roi}</h4>\n                    {/* <h5>Investments: {this.state.project.investments.amount}</h5> */}\n                    <button onClick={() => this.localClickHandler()}>Invest!</button>\n                    {this.state.investClicked ?\n                        <InvestmentForm project={this.state.project} addInvestment={this.addInvestment} currentUser={this.props.currentUser}/> \n                        : null }\n                    <ul>\n                        {this.projectInvestments()}\n                    </ul>\n            </div>\n                : <h1>Loading...</h1>}\n            </div>\n        )\n    }\n}\n\nexport default ProjectShow\n","/Users/jonathanhakimian/Desktop/Flatiron/Mod_5_Project/frontend/src/components/InvestmentForm.js",[],"/Users/jonathanhakimian/Desktop/Flatiron/Mod_5_Project/frontend/src/components/SearchForm.js",[],["92","93"],"/Users/jonathanhakimian/Desktop/Flatiron/Mod_5_Project/frontend/src/components/ProjectImageSlider.js",[],["94","95"],"/Users/jonathanhakimian/Desktop/Flatiron/Mod_5_Project/frontend/src/components/Signup.js",[],"/Users/jonathanhakimian/Desktop/Flatiron/Mod_5_Project/frontend/src/components/Login.js",["96","97"],"/Users/jonathanhakimian/Desktop/Flatiron/Mod_5_Project/frontend/src/components/UserShow.js",["98"],"\nimport React, { Component } from 'react'\nimport {Link} from 'react-router-dom'\nimport UserPieChart from './UserPieChart'\n\nclass UserShow extends Component {\n    \n    state = {\n        user: null,\n        investments: [],\n        projects: null //serializer for userProjects with breakdown by position of each project\n    }\n\n    componentDidMount() {\n        fetch(`http://localhost:3000/users/${this.props.match.params.id}`)\n        .then(response => {\n            return response.json()\n        })\n        .then(data => this.setState({user: data, investments: data.investments}))\n    }\n\n    totalPosition() {\n        let total = 0\n        if (this.state.user) {\n            let investments = this.state.investments\n            investments.forEach(element => {\n                total += element.amount\n            });\n        }\n        return total\n    }\n\n    userInvestments = () => {\n        let sortedInvestments = this.state.investments.sort((a, b) => b.id - a.id)\n        \n        return sortedInvestments.map(inv => \n                <li>\n                    {inv.date} \n                    <Link to={`/projects/${inv.project.id}`}>{inv.project.developer_name}</Link> \n                    Amount: ${inv.amount}\n                    {this.props.currentUser.id === inv.user_id ?\n                        <button onClick={this.onDelete} value={inv.id}>Delete Investment</button>\n                        : null\n                    }\n                    {/* <button onClick={this.onDelete} value={inv.id}>Delete Investment</button> */}\n                </li>\n            )\n    }\n\n    onDelete = (e) => {  \n        let id = e.target.value\n        let newInvestments = this.state.investments.filter(inv => inv.id != id)\n        this.setState({investments: newInvestments})\n\n        fetch(`http://localhost:3000/investments/${id}`, {\n            method: \"DELETE\",\n        })\n    }\n    \n    render() {\n        console.log(this.state)\n        return (\n            <div>\n                {this.state.user ?\n                    <div>\n                        <h1>User Show</h1>\n                        <h1>User: {this.state.user.username}</h1>\n                        <h2>Total Position: {this.totalPosition()}</h2>\n                        <UserPieChart investments={this.state.investments}/>\n                        {this.userInvestments()}\n                    </div> \n\n                : <h1>Loading ...</h1> }\n\n            </div>\n        )\n    }\n}\n\nexport default  UserShow\n","/Users/jonathanhakimian/Desktop/Flatiron/Mod_5_Project/frontend/src/components/UserPieChart.js",[],"/Users/jonathanhakimian/Desktop/Flatiron/Mod_5_Project/frontend/src/components/NavBar.js",["99"],"/Users/jonathanhakimian/Desktop/Flatiron/Mod_5_Project/frontend/src/components/Map.js",[],"/Users/jonathanhakimian/Desktop/Flatiron/Mod_5_Project/frontend/src/components/mapStyles.js",["100"],"export default [\n    {\