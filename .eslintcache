 \"DELETE\",\n        })\n  \n    }\n\n    percent = () => {\n        let percent \n        percent = (this.capital_raised()) / (this.state.project.total_capital_needed) * 100 \n        return percent.toString()\n    }\n\n    addInvestment = (investmentObj) => {\n        this.setState({investments: [investmentObj, ...this.state.investments]})\n    }\n\n    render() {\n        console.log(this.state)\n\n        return (\n            <div>\n            {this.state.project ?  \n            <div>\n                    <h1>Project Show</h1>\n                    <ProjectImageSlider project={this.state.project}/>\n                    <h1>{this.state.project.developer_name}</h1>\n                    <h2>Location: {this.state.project.location}</h2>\n                    <Line percent={this.percent()} strokeWidth=\"4\" strokeColor=\"green\" trailWidth=\"4\" />\n                    <h3>Total Capital Needed: {this.state.project.total_capital_needed}</h3>\n                    <h4>Captial Raised: {this.capital_raised()}</h4>\n                    <h4>Minimum Investment: {this.state.project.minimum_investment}</h4>\n                    <h4>Projected ROI: {this.state.project.projected_roi}</h4>\n                    {/* <h5>Investments: {this.state.project.investments.amount}</h5> */}\n                    <button onClick={() => this.localClickHandler()}>Invest!</button>\n                    {this.state.investClicked ?\n                        <InvestmentForm project={this.state.project} addInvestment={this.addInvestment}/> \n                        : null }\n                    <ul>\n                        {/* {this.state.project.investments.map(inv => \n                            <>\n                                <li>{inv.date} Amount: {inv.amount}\n                                <button onClick={this.onDelete} value={inv.id}>Delete Investment</button>\n                                </li>\n                            </>\n                        )} */}\n                        {this.projectInvestments()}\n                    </ul>\n            </div>\n                : <h1>Loading...</h1>}\n            </div>\n        )\n    }\n}\n\nexport default ProjectShow\n","/Users/jonathanhakimian/Desktop/Flatiron/Mod_5_Project/frontend/src/components/InvestmentForm.js",[],"/Users/jonathanhakimian/Desktop/Flatiron/Mod_5_Project/frontend/src/components/SearchForm.js",[],{"ruleId":"55","replacedBy":"56"},{"ruleId":"57","replacedBy":"58"},{"ruleId":"55","replacedBy":"59"},{"ruleId":"57","replacedBy":"60"},{"ruleId":"55","replacedBy":"61"},{"ruleId":"57","replacedBy":"62"},{"ruleId":"63","severity":1,"message":"64","line":4,"column":8,"nodeType":"65","messageId":"66","endLine":4,"endColumn":19},{"ruleId":"63","severity":1,"message":"67","line":2,"column":17,"nodeType":"65","messageId":"66","endLine":2,"endColumn":26},{"ruleId":"63","severity":1,"message":"68","line":3,"column":9,"nodeType":"65","messageId":"66","endLine":3,"endColumn":18},{"ruleId":"63","severity":1,"message":"69","line":5,"column":16,"nodeType":"65","messageId":"66","endLine":5,"endColumn":22},{"ruleId":"63","severity":1,"message":"70","line":7,"column":8,"nodeType":"65","messageId":"66","endLine":7,"endColumn":25},{"ruleId":"71","severity":1,"message":"72","line":52,"column":74,"nodeType":"73","messageId":"74","endLine":52,"endColumn":76},"no-native-reassign",["75"],"no-negated-in-lhs",["76"],["75"],["76"],["75"],["76"],"no-unused-vars","'ProjectShow' is defined but never used.","Identifier","unusedVar","'Component' is defined but never used.","'useParams' is defined but never used.","'Circle' is defined but never used.","'SimpleImageSlider' is defined but never used.","eqeqeq","Expected '!==' and instead saw '!='.","BinaryExpression","unexpected","no-global-assign","no-unsafe-negation"]